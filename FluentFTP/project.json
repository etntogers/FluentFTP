  "version": "16.2.3",
  "dependencies": {
    "NETStandard.Library": "1.6.0"
  },
  "frameworks": {
    "netstandard1.6": {
      "imports": "dnxcore50"
    },
	"dnxcore50": {
      "dependencies": {
        "System.IO": "4.3.0.0",
        "System.Net.NameResolution": "4.3.0.0",
        "System.Net.Sockets": "4.3.0.0",
        "System.Net.Security": "4.3.0.0"
      }
    }
  },
  "buildOptions": {
    "define": [ "CORE" ]
  }
}
